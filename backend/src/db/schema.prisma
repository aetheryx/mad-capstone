datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String

  @@map("user")
}

model Conversation {
  id Int @id @default(autoincrement())

  participantA Int @map("participant_a")
  participantB Int @map("participant_b")

  @@map("conversation")
}
